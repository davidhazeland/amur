    "react": "15.1.0",
    "react-autocomplete": "1.0.1",
    "react-bootstrap-table": "2.3.4",
    "react-document-title": "2.0.1",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
