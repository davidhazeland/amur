{
  "name": "react-redux-seeder",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "bootstrap-loader": "^2.0.0",
    "bootstrap-sass": "^3.3.7",
    "custom-semantic-ui": "git+ssh://git@github.com/ironnext/custom-semantic-ui.git",
    "deep-freeze": "0.0.1",
    "dom-scroll-into-view": "^1.2.1",
    "dot-prop-immutable": "1.3.1",
    "ironlake": "git+ssh://git@github.com/ironnext/ironlake.git",
    "lodash": "4.17.4",
    "mathjs": "^3.10.1",
    "moment": "2.17.1",
    "normalize.css": "^6.0.0",
    "react": "15.4.2",
    "react-bootstrap-table": "^3.1.1",
    "react-components": "git+ssh://git@github.com/ironnext/react-components.git",
    "react-document-title": "2.0.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-redux-i18n": "^1.8.0",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-form": "6.5.0",
    "redux-logger": "2.8.1",
    "redux-saga": "0.14.3",
    "redux-thunk": "2.2.0",
    "semantic-ui-react": "0.67.0",
    "store": "2.0.3",
    "style-loader": "^0.16.0",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "react-scripts": "0.9.3"
  },
  "scripts": {
    "start:js": "NODE_PATH=src/ react-scripts start",
    "start": "NODE_PATH=src/ npm-run-all -p watch:css start:js",
    "build": "NODE_PATH=src/ npm run build:css && NODE_PATH=src/ react-scripts build",
    "test": "NODE_PATH=src/ react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ --watch --recursive",
    "up:csu": "npm install git+ssh://git@github.com/ironnext/custom-semantic-ui.git",
    "up:rc": "npm install git+ssh://git@github.com/ironnext/react-components.git",
    "up:i": "npm install git+ssh://git@github.com/ironnext/ironlake.git",
    "up:all": "npm run up:csu && npm run up:rc && npm run up:i",
    "deploy": "npm run build && firebase deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
